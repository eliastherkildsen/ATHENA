<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.apollo.template.Controller.Booking.BookingCompleteController">
   <children>
      <VBox alignment="CENTER" prefHeight="400.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" text="Lokale 301" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="36.0" />
               </font>
            </Label>
            <VBox opacity="0.8" prefWidth="100.0" spacing="10.0" style="-fx-background-color: #009FE3;" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets bottom="20.0" left="50.0" right="50.0" top="30.0" />
               </VBox.margin>
               <children>
                  <Label fx:id="label_status" prefWidth="469.0" text="Succes!" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="32.0" />
                     </font>
                  </Label>
                  <Label fx:id="label_bookerName" layoutX="40.0" layoutY="40.0" prefWidth="469.0" text="navn" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="32.0" />
                     </font>
                  </Label>
                  <Label fx:id="label_statusMessage" layoutX="40.0" layoutY="20.0" text="Lokal er nu booket for dit møde:" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="32.0" />
                     </font>
                  </Label>
                  <VBox prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <Label fx:id="label_meetingType" text="MØDE TYPE" textFill="#d9d9d9">
                           <font>
                              <Font name="System Bold" size="24.0" />
                           </font>
                        </Label>
                        <Label fx:id="label_meetingTime" text="MØDE INTERVAL" textFill="#d9d9d9">
                           <font>
                              <Font name="System Bold" size="24.0" />
                           </font>
                        </Label>
                        <Label fx:id="label_meetingDate" text="MØDE DATO" textFill="#d9d9d9">
                           <font>
                              <Font name="System Bold" size="24.0" />
                           </font>
                        </Label>
                     </children>
                  </VBox>
                  <HBox alignment="BOTTOM_RIGHT" prefHeight="100.0" prefWidth="408.0">
                     <children>
                        <Button fx:id="button_back" mnemonicParsing="false" onAction="#onButton_Back" text="Tilbage" HBox.hgrow="ALWAYS" />
                     </children>
                     <VBox.margin>
                        <Insets right="30.0" />
                     </VBox.margin>
                  </HBox>
               </children>
               <padding>
                  <Insets left="30.0" top="30.0" />
               </padding>
            </VBox>
         </children>
         <padding>
            <Insets top="30.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
